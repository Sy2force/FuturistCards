{
  "name": "futuristcards",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://sy2force.github.io/Project-react/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui --project=webkit",
    "test:e2e:headed": "playwright test --headed --project=webkit",
    "test:e2e:debug": "playwright test --debug --project=webkit",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "free": "bash ../free-port.sh 3000",
    "clean:start": "bash ../reset-clean.sh",
    "dev:clean": "npm run clean:start && npm run dev",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.0.16",
    "axios": "^1.3.4",
    "framer-motion": "^10.0.1",
    "html2canvas": "^1.4.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.0",
