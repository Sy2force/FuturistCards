# Production Environment Variables - FuturistCards Frontend
VITE_API_URL=https://your-api-domain.com/api
VITE_APP_NAME=FuturistCards
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Digital Business Cards Platform

# Production Settings
VITE_NODE_ENV=production
VITE_DEBUG=false

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SENTRY=true

# UI Settings
VITE_DEFAULT_THEME=dark
VITE_DEFAULT_LANGUAGE=fr

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# Analytics (if using Google Analytics)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Sentry (if using error tracking)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# CDN Configuration
VITE_CDN_URL=https://cdn.your-domain.com
VITE_ASSETS_URL=https://assets.your-domain.com
